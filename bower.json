{
  "name": "habanero-prototype-boilerplate",
  "version": "2.0.0",
  "homepage": "https://github.com/habaneroconsulting/habanero-prototype-boilerplate",
  "authors": [
    "Christopher Parsons <cparsons@habaneroconsulting.com>"
  ],
  "license": "MIT",
  "private": true,
  "dependencies": {
    "habanero-style-utilities": "~0.3.0",
    "jquery": "^2.1.4",
    "normalize.css": "~3.0.3"
  },
  "install": {
    "path": {
      "css": "source/styles/vendor",
      "js": "source/scripts/vendor",
      "sass": "source/styles/reference/{version}"
    },
    "sources": {
      "habanero-style-utilities": "bower_components/habanero-style-utilities/reference/**",
      "jquery": "bower_components/jquery/dist/jquery.js",
      "normalize.css": "bower_components/normalize.css/normalize.css"
    }
  }
}
